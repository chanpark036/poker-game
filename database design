GameState table
{
    playerIds
    roomId
    cardsById
    currentTurnPlayerIndex
    phase
    playCount
    winningPlayers

}

Players table
{
    _id 
    name 
    age 
    earnings 
    profilePic 
    gamesPlayed 
    leaderboardRanking
}

Card table
{
    _id 
    picture 
    rank 
    suit 
    locationType (unused, table, player-hand)
    playerIndex
    
}

Room table
{
    _id
    playerIds
}